#!/bin/bash

###
export VAGRANT_EXPERIMENTAL="disks"
#export VAGRANT_DEFAULT_PROVIDER=virtualbox

export baseDIR=~/infrastructure/vagrant

### Server list file for your lab and Vagrant file
export VAGRANT_SERVER_YAML=$baseDIR/config/rhce-lab-config.yml

### Vagrant template
export VAGRANT_VAGRANTFILE=$baseDIR/templates/Vagrant-rhce-lab

### Define the default vagrant user for ssh to lab
export vagrant_user=root
